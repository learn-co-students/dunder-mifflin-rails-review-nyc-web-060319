<h1>Editing <%=@employee.first_name%></h1>

<%=form_for(@employee) do |f|%>
<%=f.label 'first name'%>
<%=f.text_field 'first_name'%>
<%=f.label 'last name'%>
<%=f.text_field 'last_name'%>
<%=f.label 'alias'%>
<%=f.text_field 'alias'%>
<%=f.label 'title'%>
<%=f.text_field 'title'%>
<%=f.label 'office'%>
<%=f.text_field 'office'%>
<%=select_tag 'dog_id', options_from_collection_for_select(Dog.all, 'id', 'name')%>
<%=f.submit 'Submit changes'%>

<%end%>